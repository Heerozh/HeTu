[run]
branch = True
source = hetu
omit =
    */site-packages/*
    site-packages/*
    hetu/cli/*
    hetu/safelogging/*
    hetu/data/default_migration.py

[html]
directory = coverage

[report]
exclude_lines =
    no cov
    no qa
    noqa
    NOQA
    pragma: no cover
    TYPE_CHECKING
    raise NotImplementedError
skip_empty = True